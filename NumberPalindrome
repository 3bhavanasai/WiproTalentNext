import java.util.*;
class NumberPalindrome
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int sum=0;
		int rem;
		System.out.println("enter a number: ");
		int a=sc.nextInt();
		int b=a;
		while(a>0){
		    rem=a%10;
		    sum=sum*10+rem;
		    a=a/10;
		}
		if(b==sum){
		    System.out.println("palindrome");
		}
		else{
		    System.out.println("not palindrome");
		}
	}
}
