import java.util.*;
class prime
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter a number: ");
		int a=sc.nextInt();
		for(int i=2;i<a;i++){
			if(a%i==0){
				System.out.println("Not a prime number");
				break;
			}
			else{
				System.out.println("Prime number");
				break;
			}
		}
	}
}
